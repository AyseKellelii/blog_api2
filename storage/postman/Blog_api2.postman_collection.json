							"const response = pm.response.json();\r",
							"\r",
							"if (response.token) {\r",
							"    pm.environment.set(\"token\", response.token);\r",
							"    console.log(\" Token kaydedildi:\", response.token);\r",
							"} else if (response.data && response.data.token) {\r",
							"    pm.environment.set(\"token\", response.data.token);\r",
							"    console.log(\"Token kaydedildi (data içinde):\", response.data.token);\r",
